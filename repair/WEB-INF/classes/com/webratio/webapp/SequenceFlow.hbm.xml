    <id name="source_oid" column="`oid`" type="java.lang.Integer"/>
    <property name="target_oid" column="`activitytype_oid`" type="java.lang.Integer" unique="false" optimistic-lock="true" lazy="false" generated="never"/>
  </class>
  <!-- mapping for relationship 'previousActivityType[nextSequenceFlow2PreviousActivityType]' -->
  <class entity-name="com.webratio.webapp.Rel_SequenceFlow_to_previousActivityType" table="`sequenceflow`" dynamic-update="true" dynamic-insert="true" mutable="true" polymorphism="implicit" select-before-update="false" optimistic-lock="version">
